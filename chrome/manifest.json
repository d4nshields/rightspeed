{
  "manifest_version": 2,

  "name": "YouTube Playback Speeder-Upper",
  "description": "This extension provides a basic slider that speeds up or slows down the HTML5 YouTube video player.",
  "version": "1.0",

  "icons": {
      "128": "stopwatch-small-128.png"
  },
  "content_scripts": [{
      "matches": ["*://*.youtube.com/watch?*"],
      "js": ["jquery.js", "jquery-ui.js", "myscript.js"],
      "run_at": "document_end"
  }],
  "browser_action": {
    "default_icon": {
      "19": "stopwatch-small-128.png",
      "38": "stopwatch-small-128.png"
    },
    "default_title": "YouTube Speeder-Upper",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
      "jquery-ui.css", "jquery.js", "jquery-ui.js", "myscript.js", "commLink.js", "stopwatch-top.png"]
}
