{
  "manifest_version": 2,

  "name": "YouTube Playback Speeder-Upper",
  "description": "Speed up or slow down any YouTube video which can be played in the HTML5 player.  Customize your viewing experience to save time, or to simply enhance your viewing pleasure.",
  "version": "0.0.0.1",

  "icons": {
      "128": "stopwatch-small-128.png"
  },
  "content_scripts": [{
      "matches": ["*://*.youtube.com/*"],
      "js": ["jquery.min.js", "jquery-ui.js", "myscript.js"],
      "run_at": "document_end"
  }],
  "browser_action": {
    "default_icon": {
      "19": "stopwatch-small-128.png",
      "38": "stopwatch-small-128.png"
    },
    "default_title": "YouTube Speeder-Upper",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
    "index.html",
      "jquery-ui.css", "spdr-styles.css", "jquery.min.js", "jquery-ui.js", "myscript.js", "commLink.js", "stopwatch-top.png"]
}
