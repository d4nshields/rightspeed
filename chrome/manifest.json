{
  "manifest_version": 2,

  "name": "YouTube Playback Speeder-Upper",
  "description": "This extension provides a basic slider that speeds up or slows down the HTML5 YouTube video player.",
  "version": "1.0",

  "content_scripts": [{
      "matches": ["http://*.youtube.com/*"],
      "css": ["jquery-ui.css"],
      "js": ["jquery.min.js", "jquery-ui.js", "myscript.js"]
  }],
  "browser_action": {
    "default_icon": {
      "19": "youtube-icon.png",
      "38": "youtube-icon.png"
    },
    "default_title": "YouTube Speeder-Upper",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}